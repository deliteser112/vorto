displayname "EncryptionProtocol"
description "Types of Wi-Fi encryption protocols"

enum EncryptionProtocol {
    WEP  "Wired Equivalent Privacy",
    WPA  "Wi-Fi Protected Access",
    WPA2 "Wi-Fi Protected Access Version 2",
    NONE "Open wireless network",
    AUTO "Dynamic Encryption"
}