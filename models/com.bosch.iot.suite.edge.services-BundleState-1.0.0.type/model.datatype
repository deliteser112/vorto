vortolang 1.0

namespace com.bosch.iot.suite.edge.services
version 1.0.0
displayname "BundleState"
description "Represents the possible states of an OSGi bundle."

enum BundleState {
	INSTALLED "The bundle is installed but not resolved yet.",
	RESOLVED "The bundle is resolved and is able to be started.",
	ACTIVE "The bundle is now running.",
	STARTING "The bundle is in the process of starting.",
	STOPPING "The bundle is in the process of stopping."
}